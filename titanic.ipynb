    "ano_base = 1912\n",
    "\n",
    "df = df.fillna(0)\n",
    "\n",
    "df['Nasc.'] = ano_base - df['Idade'].astype(int)\n",
    "\n",
    "df = df.sort_values(by=\"Nasc.\", ascending=False)\n",
    "\n",
    "df[['Nome', 'Nasc.', \"Idade\"]]"
